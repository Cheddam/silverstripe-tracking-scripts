{
    "name": "springtimesoft/silverstripe-tracking-scripts",
    "description": "A module to add various tracking scripts to Silverstripe",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/springtimesoft/silverstripe-tracking-scripts",
    "keywords": [
        "silverstripe",
        "tracking",
        "analytics",
        "google",
        "facebook"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Springtimesoft",
            "homepage": "https://springtimesoft.co.nz/"
        }
    ],
    "support": {
        "issues": "https://github.com/springtimesoft/silverstripe-tracking-scripts/issues"
    },
    "require": {
        "silverstripe/siteconfig": "^4.0 || ^5.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Springtimesoft\\TrackingScripts\\": "src/"
        }
    }
}
